import pandas as pd
import numpy as np

from catboost import CatBoostClassifier
import shap

import matplotlib.pyplot as plt
import seaborn as sns

sns.set(style="whitegrid", context="talk")
plt.rcParams["figure.figsize"] = (9, 6)

print("Interpretability libraries loaded.")

DATA_PATH = "../data/processed/testicular_cancer_engineered.csv"

df = pd.read_csv(DATA_PATH)

df.head()

feature_cols = [
    "age_at_diagnosis",
    "high_t_stage",
    "node_positive",
    "metastatic",
    "marker_elevated",
    "tmb_log"
]

X = df[feature_cols]
y = df["target"]

print("Dataset shape:", X.shape)

cat_model = CatBoostClassifier(
    iterations=500,
    learning_rate=0.05,
    depth=4,
    loss_function="Logloss",
    eval_metric="AUC",
    verbose=False,
    random_state=42
)

cat_model.fit(X, y)

print("CatBoost model refit on full dataset.")

explainer = shap.TreeExplainer(cat_model)
shap_values = explainer.shap_values(X)

print("SHAP values computed.")

shap.summary_plot(
    shap_values,
    X,
    plot_type="bar",
    show=True
)

shap.summary_plot(
    shap_values,
    X,
    show=True
)

for feature in ["marker_elevated", "high_t_stage", "node_positive"]:
    shap.dependence_plot(
        feature,
        shap_values,
        X,
        show=True
    )

# Select an example patient with residual tumor
example_index = df[df["target"] == 1].index[0]

shap.force_plot(
    explainer.expected_value,
    shap_values[example_index],
    X.iloc[example_index],
    matplotlib=True
)

shap_df = pd.DataFrame(shap_values, columns=feature_cols)
shap_df["target"] = y.values

mean_shap = shap_df.groupby("target").mean()
mean_shap

mean_shap.T.plot(kind="bar")
plt.title("Mean SHAP Values by Outcome Group")
plt.ylabel("Mean SHAP Value")
plt.xlabel("Feature")
plt.xticks(rotation=45)
plt.legend(["Tumor Free", "With Tumor"])
plt.show()

interpretation_summary = pd.DataFrame({
    "feature": feature_cols,
    "clinical_role": [
        "Demographic baseline risk",
        "Local tumor invasion",
        "Regional lymphatic spread",
        "Distant metastasis",
        "Biochemical tumor activity",
        "Molecular mutational load"
    ],
    "model_importance": shap_df[feature_cols].abs().mean().values
}).sort_values("model_importance", ascending=False)

interpretation_summary

