import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns

sns.set(style="whitegrid", context="talk")
plt.rcParams["figure.figsize"] = (10, 6)

print("EDA libraries loaded.")

DATA_PATH = "../data/processed/testicular_cancer_cleaned.csv"

df = pd.read_csv(DATA_PATH)

print(f"Dataset loaded: {df.shape}")

df.head()

df.describe(include="all").transpose()

sns.histplot(df["age_at_diagnosis"], bins=20, kde=True)
plt.title("Age at Diagnosis Distribution")
plt.xlabel("Age (years)")
plt.ylabel("Patient Count")
plt.show()

stage_counts = df["ajcc_stage"].value_counts(dropna=False)

stage_counts

sns.barplot(x=stage_counts.index, y=stage_counts.values)
plt.title("AJCC Pathologic Stage Distribution")
plt.xlabel("AJCC Stage")
plt.ylabel("Number of Patients")
plt.xticks(rotation=45)
plt.show()

fig, axes = plt.subplots(1, 3, figsize=(18, 5))

sns.countplot(x=df["ajcc_t_stage"], ax=axes[0])
axes[0].set_title("T Stage Distribution")
axes[0].tick_params(axis="x", rotation=45)

sns.countplot(x=df["ajcc_n_stage"], ax=axes[1])
axes[1].set_title("N Stage Distribution")
axes[1].tick_params(axis="x", rotation=45)

sns.countplot(x=df["ajcc_m_stage"], ax=axes[2])
axes[2].set_title("M Stage Distribution")
axes[2].tick_params(axis="x", rotation=45)

plt.tight_layout()
plt.show()

marker_counts = df["serum_marker_stage"].value_counts(dropna=False)

marker_counts

sns.barplot(x=marker_counts.index, y=marker_counts.values)
plt.title("Serum Marker Stage Distribution")
plt.xlabel("Serum Marker Stage")
plt.ylabel("Number of Patients")
plt.xticks(rotation=45)
plt.show()

sns.histplot(df["tumor_mutational_burden"], bins=20, kde=True)
plt.title("Tumor Mutational Burden Distribution")
plt.xlabel("TMB (mutations / Mb)")
plt.ylabel("Patient Count")
plt.show()

df["tumor_mutational_burden"].describe()

outcome_counts = df["disease_status"].value_counts()

outcome_counts

sns.barplot(x=outcome_counts.index, y=outcome_counts.values)
plt.title("Disease Status at Follow-up")
plt.xlabel("Disease Status")
plt.ylabel("Number of Patients")
plt.show()

stage_outcome = pd.crosstab(df["ajcc_stage"], df["disease_status"])

stage_outcome

stage_outcome.plot(kind="bar", stacked=True)
plt.title("AJCC Stage vs Disease Status")
plt.xlabel("AJCC Stage")
plt.ylabel("Patient Count")
plt.xticks(rotation=45)
plt.legend(title="Disease Status")
plt.show()

numeric_df = df.select_dtypes(include=[np.number])

corr = numeric_df.corr()

sns.heatmap(corr, annot=True, cmap="coolwarm", fmt=".2f")
plt.title("Correlation Heatmap (Numeric Features)")
plt.show()

sns.boxplot(x=df["ajcc_stage"], y=df["age_at_diagnosis"])
plt.title("Age at Diagnosis by AJCC Stage")
plt.xlabel("AJCC Stage")
plt.ylabel("Age")
plt.xticks(rotation=45)
plt.show()

eda_summary = {
    "median_age": df["age_at_diagnosis"].median(),
    "most_common_stage": df["ajcc_stage"].mode()[0],
    "tumor_free_rate": (df["disease_status"] == "TUMOR FREE").mean(),
    "median_tmb": df["tumor_mutational_burden"].median()
}

eda_summary
