OUTPUT_PATH = "../data/processed/testicular_cancer_cleaned.csv"

df.to_csv(OUTPUT_PATH, index=False)

print(f"Cleaned dataset saved to: {OUTPUT_PATH}")

summary = {
    "patients": len(df),
