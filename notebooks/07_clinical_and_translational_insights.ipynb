import pandas as pd
import numpy as np

import matplotlib.pyplot as plt
import seaborn as sns

sns.set(style="whitegrid", context="talk")
plt.rcParams["figure.figsize"] = (9, 6)

print("Insight synthesis libraries loaded.")

cleaned_path = "../data/processed/testicular_cancer_cleaned.csv"
engineered_path = "../data/processed/testicular_cancer_engineered.csv"

df_clean = pd.read_csv(cleaned_path)
df_eng = pd.read_csv(engineered_path)

print("Cleaned dataset:", df_clean.shape)
print("Engineered dataset:", df_eng.shape)

df = df_eng.merge(
    df_clean,
    on="patient_id",
    suffixes=("_eng", "_clin")
)

df.head()

risk_factors = [
    "high_t_stage",
    "node_positive",
    "metastatic",
    "marker_elevated"
]

risk_summary = df.groupby("target")[risk_factors].mean()
risk_summary

marker_outcome = pd.crosstab(
    df["serum_marker_stage"],
    df["target"],
    normalize="index"
)

marker_outcome

marker_outcome.plot(kind="bar", stacked=True)
plt.title("Serum Marker Stage vs Outcome")
plt.xlabel("Serum Marker Stage")
plt.ylabel("Proportion")
plt.legend(["Tumor Free", "With Tumor"])
plt.xticks(rotation=45)
plt.show()

sns.boxplot(
    x=df["target"],
    y=df["tumor_mutational_burden"]
)
plt.title("Tumor Mutational Burden by Outcome")
plt.xlabel("Disease Status (0=Tumor Free, 1=With Tumor)")
plt.ylabel("TMB")
plt.show()

sns.boxplot(
    x=df["target"],
    y=df["age_at_diagnosis"]
)
plt.title("Age at Diagnosis by Outcome")
plt.xlabel("Disease Status")
plt.ylabel("Age")
plt.show()

stage_vs_model = pd.crosstab(
    df["stage_group"],
    df["target"]
)

stage_vs_model

df["risk_group"] = "LOW"

df.loc[
    (df["marker_elevated"] == 1) |
    (df["high_t_stage"] == 1) |
    (df["node_positive"] == 1),
    "risk_group"
] = "HIGH"

df["risk_group"].value_counts()

pd.crosstab(df["risk_group"], df["target"], normalize="index")

key_findings = pd.DataFrame({
    "Finding": [
        "Most TGCT cases are early-stage and localized",
        "Serum tumor markers are the strongest predictors of residual disease",
        "TNM invasion and nodal spread drive prognosis",
        "Tumor mutational burden has limited prognostic value",
        "Machine learning aligns well with clinical staging",
        "Non-linear models outperform linear models"
    ],
    "Clinical Implication": [
        "High cure rates with early detection",
        "Biochemical monitoring is critical post-orchiectomy",
        "TNM staging remains clinically valid",
        "Immunotherapy benefit likely limited",
        "ML can support but not replace staging",
        "Tree-based models suit clinicogenomic data"
    ]
})

key_findings
